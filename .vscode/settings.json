    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "emmet.includeLanguages": {
    "html": "html"
  },
  "liveServer.settings.port": 5500,
  "liveServer.settings.root": "/"
}

