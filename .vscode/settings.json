{
  "chat.promptFilesRecommendations": {
    "add-page": true,
    "add-car": true,
    "add-component": true,
    "add-feature": true
  },
  "chat.tools.terminal.autoApprove": {
    ".specify/scripts/bash/": true
  },
  "files.associations": {
    "*.html": "html",
    "*.css": "css",
    "*.js": "javascript"
  },
